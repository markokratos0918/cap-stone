{
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-unused-vars": "off",
      "react/jsx-key": "off",
      "react/prop-types": "off",
      "react/no-unknown-property": "off",
      "react/react-in-jsx-scope": "off",
      "jsx-a11y/alt-text": "off",
      "react/display-name": "off"
    }
  }
  